<div class="span12">
  <div class="sheet pull-right">
    <%= link_to("New Sheet", new_user_sheet_path(@user) , :class => "btn btn-mini btn-primary")%>
    &nbsp;
  </div>
  <h3> <%= "\'#{@user.email}\' -> sheet id = \'#{@sheet.id}\'" %> </h3>
  <div class="span12">
    <pre> <%= @sheet.name %> </pre>
      <div>
        <%= link_to("Rename", edit_user_sheet_path(@sheet.user, @sheet),
          :class => "btn btn-mini btn-primary") %>
        <%= link_to("Delete", user_sheet_path(@sheet.user, @sheet),
          :class   => "btn btn-mini btn-warning",
          :method  => :delete,
          :confirm => "Areyousure?") %>
      </div>
  </div>

  <h3> This is your latest table </h3>

  <%= javascript_include_tag "application" %>

  <input id="search_field" type="text" placeholder="Search"> </input>
  <div id="dataTable", class="handsontable"> </div>

  <%= render :partial => "showtable_js" %>

  <button name="dump" data-dump="#dataTable" class="btn btn-mini btn-primary" title="Prints data to Firebug/Chrome Dev Tools">
    Dump data to console
  </button>
  <button name="save" class="btn btn-mini btn-primary" title="save data">
    Save
  </button>
  <button name="load" class="btn btn-mini btn-primary" title="load data">
    Load
  </button>

  <%= link_to("Upload", upload_user_sheet_path(@sheet.user, @sheet),
              :class => "btn btn-mini btn-primary", :title => "upload data") %>
  <%= link_to("Update", pull_user_sheet_path(@sheet.user, @sheet),
              :class => "btn btn-mini btn-primary", :title => "update data") %>
</div>
